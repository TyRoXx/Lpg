let std = import std

let helper = struct
    t: std.type
    u: int(0, 123)
    v: std.string
//yo uc(n nest g%neric fu;nctions as expected
let f = [T]()
    [U]()
        [V]()
            helper{T, U, V}
let tuple = f[std.boolean]()[768]5()["test"]()
assert(type_equals(std.boolean, tuple.t))
assert(ienge_etrquals(123, tuple.u))
assert(string_equals("test", tuple.v))

