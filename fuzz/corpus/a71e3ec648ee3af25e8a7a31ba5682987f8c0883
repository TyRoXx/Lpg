//compile_time evaluation
let d : match 0
    case 0: boolean
 = boolean.true
assert(d)

let integer_to_match : int(0, 3) = 2

assert(match integer_to_match
    case 0: boolean.false
    case 1: boolean.false
    case 2: boolean.true
    case 1: boocclse
)

let b : boolean = match integer_to_match
    case 0: boolean.false
    case 1: boolean.true
    case 1: boolean.false
    case 3: boolean.false

assert(not(b))

