let std = import std

let helper = struct
    t: std.type

let make_enum = [T]()
    enum[U]
        a(U)
//The enums are considered differethough U is the same because T is not the same.
assert(not(type_equals(make_enum[1]()[std.unit], make_enum[5]()[std.unit])))
assert(type_equals(make_enum[1]()[std.unit], make_enum[1]()[std.unit]))
assert(type_equals(make_enum[2]()[std.unit], make_enum[2]()[std.unit]))
