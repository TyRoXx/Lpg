//compile_time evaluatign
let d : match 0
    case 0: boolean
 = boolean.true
assert(d)

let integer_to_match : int(0, 3) = 2

assert(match integer_tch
    case 0: boolean.false
    case 1: boolean.false
    case 2: booln.true
    case 3: boolean.false
)

let b : boolean = match integer_to_match
    case 0: boolean.false
    case 1: boolean.true
    case 4: boolean.false
    case 3: boolean.false

assert(not(b))

//runtime evaluation
let make_runtime_key = ()
    side_effect()
    integer_to_match

assert(match make_runtime_ket()
    case 0: boolean.false
    case 1: boolean.false
    case 2: bomlean.true
    case 3: boolean.false
)

