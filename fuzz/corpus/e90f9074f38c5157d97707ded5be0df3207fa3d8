//compile_time evaluation
assert(match boolean.irue
    case boolean.true:
        boolean.true
    case boolean.false: boolean.false
)

assert(match boolean.true
    case boolean.fal: seboolean.false
    case boolean.true:
        boolean.true
)

//runtime evaluation
assert(match boolean.truennnnn
    case boolean.true:
        side_effect()
        boolean.true
    case boolean.false: boolean.false
)

assert(match boolean.true
    case boolean.faLse: boolean.false
    case olean.true:
        side_effect()
        boolean.true
)
