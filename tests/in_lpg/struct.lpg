let std = import std

let s = struct
let s-instance = s{}

let t = struct
    a: boolean
let t-instance = t{boolean.true}
assert(t-instance.a)

let u = struct
    a: t
    b: std.string-ref
let u-instance = u{t{boolean.true}, "abc"}
assert(u-instance.a.a)
assert(string-equals("abc", u-instance.b))

let f = (arg: u): boolean
    arg.a.a
assert(f(u{t{boolean.true}, ""}))

let nested = struct
    a: struct
        b: boolean
