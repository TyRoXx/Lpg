let std = import std
let es = import ecmascript
let option = std.option

(window: es.value, factory: es.value-factory)
    let arguments = new-array(es.value)
    assert(arguments.append(factory.create-string("Hello, world!")))
    match window.load("console")
        case option[es.value].some(let console):
            console.call-method("log", arguments)
            std.unit_value
        case option[es.value].none:
            assert(boolean.false)
            std.unit_value
    window.call-method("alert", arguments)
